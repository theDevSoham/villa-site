{
  "format": "custom",
  "id": "event_property",
  "label": "Event Property",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "property_image": {
        "type": "Image",
        "config": {
          "label": "Property Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "heading2"
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Description",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em"
        }
      },
      "event": {
        "type": "Select",
        "config": {
          "label": "Event",
          "placeholder": "",
          "options": ["corporate", "birthday", "reception"]
        }
      },
      "event_description": {
        "type": "StructuredText",
        "config": {
          "label": "Event Description",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em"
        }
      },
      "is_available": {
        "type": "Boolean",
        "config": {
          "label": "Is Available",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": false
        }
      }
    }
  }
}
